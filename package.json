{
  "name": "@yme/api",
  "description": "define type-safe API requests simply",
  "version": "2.0.0-alpha.1",
  "type": "module",
  "sideEffects": false,
  "author": "mino",
  "repository": "minosss/api",
  "keywords": ["api", "client", "rest", "http", "https"],
  "license": "MIT",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && publint --strict",
    "prepublishOnly": "bun run build",
    "release": "np",
    "test": "bun test --coverage",
    "lint": "biome lint",
    "format": "biome format --write ."
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/bun": "^1.1.8",
    "@types/node": "^22.5.4",
    "publint": "^0.2.10",
    "typescript": "^5.5.4",
		"np": "^10.0.7",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "packageManager": "bun@1.1.27",
  "engines": {
    "node": ">=18.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "1kb"
    }
  ]
}
